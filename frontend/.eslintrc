{
  "parser": "@babel/eslint-parser",
  "plugins": ["jsx-a11y", "prettier"],
  "rules": {
    "no-param-reassign": 0,
    "prettier/prettier": ["error"]
  },
  "extends": [
    "react-app",
    "esnext",
    "esnext/style-guide",
    "node",
    "node/style-guide",
    "plugin:jsx-a11y/recommended",
    "eslint:recommended",
    "prettier",
    "airbnb-base",
    "plugin:prettier/recommended",
    "prettier"
  ]
}
